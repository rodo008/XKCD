<body>
    
    
    <div class="image-container set-full-height" style="background-image: url('@Url.Content("~/assets/img/paper-2.jpeg")')">


        
        <!--   Big container   -->
        <div class="container">            
            <!--      Wizard container        -->
            <div class="wizard-container">
                <header>
                    <h1>XKCD</h1>
                </header>
                <div class="card wizard-card" data-color="red" id="wizard">
                    @RenderBody()
                </div>
            </div> <!-- wizard container -->
        </div> <!--  big container -->

        <div class="footer">
            <div class="container text-center">
                Made with <i class="fa fa-heart heart"></i> by Rodolfo J. Espinoza Lopez.
            </div>
        </div>

    </div>

</body>

